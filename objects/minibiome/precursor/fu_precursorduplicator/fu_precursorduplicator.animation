{
  "animatedParts" : {
    "stateTypes" : {
      "base" : {
        "default" : "off",
        "states" : {
          "off" : {
            "frames" : 1
          },
          "on" : {
            "frames" : 80,
            "cycle" : 3,
            "mode" : "loop"
          }
        }
      }
    },

    "parts" : {
     
      "base" : {
	"properties" : {
	  "centered" : false,
	  "zLevel" : 1,
	  "fullbright" : false
	},
        "partStates" : {
          "base" : {
            "off" : {
              "properties" : {
                "image" : "<partImage>:default.default"
              }
            },

            "on" : {
              "properties" : {
                "image" : "<partImage>:default.<frame>"
              }
            }
          }
        }
      }
    }
  }
}
